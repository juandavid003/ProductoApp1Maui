<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProductoApp1.IniciarSesion"
             Title="IniciarSesion">
    <VerticalStackLayout>
        <TableView >
            <TableRoot>
                <TableSection Title="Iniciar Sesion">
                    <EntryCell x:Name="Correo"
                             Placeholder="Correo"/>
                    <EntryCell x:Name="Contrasena"
                             Placeholder="*********"/>


                </TableSection>


            </TableRoot>
        </TableView>
        <Button Text="Iniciar Sesion"
         FontSize="15"
         Margin="5"
         Clicked="OnClickIniciarSesion"/>
    </VerticalStackLayout>
</ContentPage>